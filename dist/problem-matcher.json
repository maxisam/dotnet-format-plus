{
    "problemMatcher": [
        {
            "owner": "dotnet-format-plus",
            "pattern": [
                {
                    "regexp": "^\\s+(.*)\\((\\d+),(\\d+)\\):\\s+(.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "message": 4
                }
            ]
        }
    ]
}
