{
    "name": "dotnet-format-plus",
    "version": "0.0.0",
    "private": true,
    "description": "yet another dotnet-format action",
    "main": "lib/main.js",
    "scripts": {
        "build": "tsc",
        "format": "prettier --write \"src/**/*.ts\"",
        "format-check": "prettier --check \"src/**/*.ts\"",
        "lint": "eslint .",
        "package": "ncc build --source-map",
        "test": "jest --passWithNoTests",
        "test:w": "jest --watchAll",
        "copy-matcher": "cp ./src/problem-matcher.json ./dist/",
        "all": "npm run build && npm run format && npm run lint && npm run package && npm test && npm run copy-matcher"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/actions/typescript-action.git"
    },
    "keywords": [
        "actions",
        "github actions"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@actions/artifact": "^2.2.1",
        "@actions/core": "^1.11.1",
        "@actions/exec": "^1.1.1",
        "@actions/github": "^6.0.0",
        "@actions/io": "1.1.3",
        "@jscpd/core": "^4.0.1",
        "@jscpd/finder": "^4.0.1",
        "@jscpd/tokenizer": "^4.0.1",
        "@octokit/rest": "^21.1.0",
        "deepmerge": "4.3.1",
        "js-yaml": "^4.1.0",
        "jscpd": "^4.0.5",
        "node-fetch": "^3.3.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.26.4",
        "@babel/core": "^7.26.7",
        "@babel/preset-env": "^7.26.7",
        "@octokit/types": "^13.8.0",
        "@types/jest": "^29.5.14",
        "@types/js-yaml": "^4.0.9",
        "@types/node": "^22.13.1",
        "@types/node-fetch": "2.6.12",
        "@typescript-eslint/parser": "^8.23.0",
        "@typescript-eslint/eslint-plugin": "^8.23.0",
        "globals": "^15.14.0",
        "@vercel/ncc": "^0.38.3",
        "@eslint/js ": "^9.20.0",
        "babel-jest": "^29.7.0",
        "eslint": "^9.20.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-github": "^5.1.8",
        "eslint-plugin-jest": "^27.9.0",
        "eslint-plugin-prettier": "^5.2.3",
        "jest": "^29.7.0",
        "js-yaml": "^4.1.0",
        "prettier": "3.4.2",
        "ts-jest": "^29.2.5",
        "typescript-eslint": "^8.23.0",
        "typescript": "^4.9.5"
    },
    "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}